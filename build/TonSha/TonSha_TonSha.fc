        if (($expected != $msg'digest)) {
            $global_emit($VerificationFailed$_store_cell($VerificationFailed$_constructor_hw_id_reason($msg'hw_id, 4), begin_cell()));
            $TonSha$_contract_store(($self'owner, $self'authorized_devices, $self'approved_firmware, $self'counters));
            return ();
        }
