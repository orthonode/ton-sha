        $global_emit($VerificationPassed$_store_cell($VerificationPassed$_constructor_hw_id_counter($msg'hw_id, $msg'counter), begin_cell()));
        $TonSha$_contract_store(($self'owner, $self'authorized_devices, $self'approved_firmware, $self'counters));
